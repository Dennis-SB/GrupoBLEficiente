@model FrontEnd.Models.JobTitleViewModel

@{
    ViewData["Title"] = "Eliminar";
}

<h1>Eliminar</h1>

<h3>¿Está seguro que desea elimninar este registro?</h3>
<div>
    <hr />
    <dl class="row" style="margin-left: 1%;">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdJobTitle)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdJobTitle)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        @Html.HiddenFor(Model => Model.IdJobTitle)
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la Lista</a>
    </form>
</div>